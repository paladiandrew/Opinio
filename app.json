{
    "expo": {
      "name": "opinio",
      "slug": "opinio",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/images/icon.png",
      "scheme": "vk51950862",
      "userInterfaceStyle": "automatic",
      "androidNavigationBar": {
        "visible": "immersive"
      },
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "ios": {
        "supportsTablet": true
      },
      "android": {
        "adaptiveIcon": {
          "foregroundImage": "./assets/images/adaptive-icon.png",
          "backgroundColor": "#ffffff"
        },
        "intentFilters": [
  {
    "action": "VIEW",
    "data": [
      {
        "scheme": "vk51950862",
        "host": "vk.com",
        "pathPrefix": "/blank.html"
      }
    ],
    "category": [
      "BROWSABLE",
      "DEFAULT"
    ]
  }
],

        "package": "com.anonymous.testopinio",
        "config": {
          "googleSignIn": {
            "certificateHash": "02:FC:90:8E:36:19:CE:C5:87:91:26:B8:8C:39:CF:69"
          }
        }
      },
      "web": {
        "config": "metro.config.js",
        "bundler": "metro",
        "output": "static",
        "favicon": "./assets/images/favicon.png"
      },
      "plugins": [
        "expo-router",
        [
          "expo-font",
          {
            "fonts": [
              "./assets/fonts/NunitoSans-ExtraBold.ttf",
              "./assets/fonts/Roboto-Medium.ttf",
              "./assets/fonts/Montserrat-Italic-VariableFont_wght.ttf"
            ]
          }
        ],
        [
            "expo-image-picker",
            {
              "photosPermission": "The app accesses your photos to let you share them with your friends."
            }
          ]
      ],
      "experiments": {
        "typedRoutes": true
      },
      "extra": {
        "router": {
          "origin": false
        },
        "eas": {
          "projectId": "8fc5b201-b527-4481-857d-e3fa1b5076e0"
        }
      }
    }
  }